{
  "name": "main-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "app:command": "ts-node --swc --files src/cli.ts",
    "tinker": "nest start --entryFile tinker"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/sequelize": "^11.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "connect-flash": "^0.1.1",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "moment": "^2.30.1",
    "nestjs-command": "^3.1.5",
    "nestjs-form-data": "^1.9.93",
    "nestjs-sequelize-paginate": "^1.0.0",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "session-file-store": "^1.5.0",
    "swagger-ui-express": "^5.0.1",
    "ts-enum-util": "^4.1.0",
    "twig": "^1.17.1",
    "umzug": "^3.8.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.11.31",
    "@swc/jest": "^0.2.38",
    "@types/bcrypt": "^5.0.2",
    "@types/connect-flash": "^0.0.40",
    "@types/express": "^5.0.2",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.30",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.4",
    "@types/sequelize": "^4.28.20",
    "@types/session-file-store": "^1.2.5",
    "@types/supertest": "^6.0.3",
    "@types/twig": "^1.12.17",
    "@types/umzug": "^2.3.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "jest": "29.7.0",
    "mockdate": "^3.0.5",
    "nestjs-cls": "^6.0.1",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "@swc/jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.module.(t|j)s",
      "!databases/migrations/*.(t|j)s",
      "!databases/models/**/*.(t|j)s",
      "!databases/config.(t|j)s",
      "!documentation-helper/**/*.(t|j)s",
      "!environment/configs/*.(t|j)s",
      "!main.(t|j)s",
      "!tinker.(t|j)s",
      "!cli.(t|j)s",
      "!migration.(t|j)s",
      "!cli-commands/**/*.(t|j)s",
      "!cluster/**/*.(t|j)s",
      "!helpers/filters/error-validation-format/**/*.(t|j)s",
      "!helpers/utils/check-env-file.(t|j)s",
      "!helpers/interceptors/context/**/*.(t|j)s",
      "!health/controllers/health/**/*.(t|j)s",
      "!common/services/transaction-provider/**/*.(t|j)s",
      "!services/logging/**/*.(t|j)s",
      "!services/url-config-service/**/*.(t|j)s",
      "!mail/services/mail-config/**/*.(t|j)s",
      "!mail/services/mail-config/**/*.(t|j)s",
      "!transaction-manager/services/transaction-provider/**/*.(t|j)s",
      "!view-engine/services/view-engine-config/**/*.(t|j)s",
      "!session-manager/services/session-config/**/*.(t|j)s",
      "!**/*.decorator.(t|j)s",
      "!paginate-overwrite/services/overwrite-paginate.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
